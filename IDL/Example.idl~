#include "orbsvcs/TimeBase.idl"

module ExampleApp
{
	#pragma DCPS_DATA_TYPE "ExampleApp::Thing"
	#pragma DCPS_DATA_KEY "ExampleApp::Thing name"
	
	struct Data
	{
		string name;
		string test;
		
		double value;
		
		TimeBase::TimeT timestamp;
	};
	
	#pragma DCPS_DATA_TYPE "ExampleApp::Event"
	#pragma DCPS_DATA_KEY "ExampleApp::Event kicker"
	
	enum EventType
	{
		Run,
		Stop,
		Stuck
	};
	
	struct Event
	{
		string kicker;
		EventType event;
		TimeBase::TimeT timestamp;
	};
};
	 